#!/usr/bin/env bash


function caskinstall(){
  brew cask install "${@}" 2> /dev/null
}

brew cask upgrade


caskinstall keepassx
caskinstall font-menlo-for-powerline 
caskinstall p4merge

caskinstall qlprettypatch

caskinstall qlcolorcode
caskinstall qlmarkdown
caskinstall quicklook-json
caskinstall quicklook-csv
caskinstall webpquicklook
caskinstall betterzip
caskinstall qsyncthingtray

caskinstall imageoptim
caskinstall image2icon
caskinstall vlc

caskinstall firefox
caskinstall firefox-developer-edition
caskinstall firefox-nightly
caskinstall google-chrome
caskinstall google-chrome-canary
caskinstall opera
caskinstall chromium

caskinstall discord 
caskinstall dropbox

caskinstall tunnelblick
caskinstall sketch
caskinstall arduino
caskinstall patchwork
caskinstall webtorrent

caskinstall sourcetree
caskinstall virtualbox
caskinstall openscad
caskinstall vscodium 
cask cleanup
